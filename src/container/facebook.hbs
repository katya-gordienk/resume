        {{#if groups}}
        <br>
        {{#each groups}}
        <br>
        <li>(id: {{id}}), {{name}} {{description}}</li>
        <br>
        <b> Members</b>
        {{#if members}}
        <br>
        {{#each members}}
        <br>
        (id: {{id}}), {{name}} {{gender}}, {{age}}
        {{/each}}
        {{/if}}

        {{/each}}
        {{/if}}

        {{/each}}
        {{/if}}
</div>
</main>
</div>