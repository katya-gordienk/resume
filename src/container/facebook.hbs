        <ul>
            <li>(id: {{id}})
                {{name}}, {{gender}}, {{age}} </li>
        </ul>
        <br>
        <b>Friends</b>
        {{#if friends}}
        {{#each friends}}
        <li>(id: {{id}}), {{name}} {{gender}}, {{age}} </li>
