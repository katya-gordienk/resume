{{!--
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<div>
    <main>

        <h3> {{> title data='Web:'big=true }}</h3>
        {{#with web}}
        {{#each languages }}
        <ul>
            <li><em>{{name}}</em> </li>
            <li> <em>version:</em> ({{ version}})</li>
            <br>
            <p> <em>description:</em>
            <div></div> <b> {{description}}</b> </p>
            <br>

            {{#each elements}}
            <ul>
                <li> <em>{{name}}</em> - <b> {{ description}}</b></li>
                <br>
                {{#each attributes}}
                <ul>
                    <li><em>{{name}}</em> - <b> {{ description}}</b> </li>
                    <br>
                </ul>

                {{/each}}
            </ul>

            {{/each}}
            {{#each properties}}
            <ul>
                <li> <em>{{name}}</em>- <b>{{ description}}</b> </li>
                <br>
                {{#if values}}
                <ul>
                    {{#each values}}
                    <li>{{value}}: {{description}}</li>

                    {{/each}}
                    {{/if}}
                </ul>

                {{#each values}}
                <em>{{name}}</em>
                <br>
                <em>{{type}}</em>
                <br>
                <b>{{description}}</b>


                <br>
                {{#each functions}}
                <ul>
                    <li><em>{{name}}</em>- <b>{{ description}}</b> </li>
                    {{#each parameters}}
                    <ul>
                        <li></li>
                    </ul>
                    {{/each}}
                    {{/each}}
                </ul>
                {{/each}}
                {{/each}}
            </ul>
            {{/each}}
        </ul>
        {{/with}}

    </main>
</div>


{{!--
Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
Адже наш layout/default.hbs має вже структуру HTML
--}}